{
  if (data == null)   return "";
  StringBuffer sb=new StringBuffer();
  Map<String,String> params=data.toMap();
  for (  Entry<String,String> s : params.entrySet()) {
    sb.append('&');
    sb.append(URLEncoder.encode(s.getKey(),ENCODE) + "=" + URLEncoder.encode(s.getValue(),ENCODE));
  }
  return sb.toString();
}
