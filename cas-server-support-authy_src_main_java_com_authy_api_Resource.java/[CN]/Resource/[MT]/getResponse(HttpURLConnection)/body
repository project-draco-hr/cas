{
  InputStream in=null;
  if (status != 200) {
    in=connection.getErrorStream();
  }
 else {
    in=connection.getInputStream();
  }
  BufferedInputStream input=new BufferedInputStream(in);
  StringBuffer sb=new StringBuffer();
  int ch;
  while ((ch=input.read()) != -1) {
    sb.append((char)ch);
  }
  input.close();
  return sb.toString();
}
