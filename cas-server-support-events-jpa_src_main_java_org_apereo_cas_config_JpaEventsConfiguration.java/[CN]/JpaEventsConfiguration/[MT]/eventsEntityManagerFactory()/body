{
  final LocalContainerEntityManagerFactoryBean bean=Beans.newEntityManagerFactoryBean(new JpaConfigDataHolder(jpaEventVendorAdapter(),"jpaEventRegistryContext",jpaEventPackagesToScan(),dataSourceEvent()),casProperties.getEvents().getJpa().getDatabase());
  bean.getJpaPropertyMap().put("hibernate.enable_lazy_load_no_trans",Boolean.TRUE);
  return bean;
}
