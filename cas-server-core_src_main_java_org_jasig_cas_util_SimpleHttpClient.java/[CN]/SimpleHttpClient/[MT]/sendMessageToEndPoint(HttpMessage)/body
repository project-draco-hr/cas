{
  final Callable<Boolean> callable=new CallableHttpMessageSender(message);
  final Future<Boolean> result=EXECUTOR_SERVICE.submit(callable);
  if (message.isIssueAsynchronousCallbacks()) {
    return true;
  }
  try {
    return result.get();
  }
 catch (  final Exception e) {
    return false;
  }
}
