{
  final OidcServerDiscoverySettings discoveryProperties=new OidcServerDiscoverySettings(casProperties.getServer().getPrefix(),casProperties.getAuthn().getOidc().getIssuer());
  discoveryProperties.setSupportedClaims(ImmutableList.of(OidcConstants.CLAIM_SUB,"name",OidcConstants.CLAIM_PREFERRED_USERNAME,"family_name","given_name","middle_name","given_name","profile","picture","nickname","website","zoneinfo","locale","updated_at","birthdate","email","email_verified","phone_number","phone_number_verified","address"));
  discoveryProperties.setSupportedScopes(OidcConstants.SCOPES);
  discoveryProperties.setSupportedResponseTypes(ImmutableList.of("code","token"));
  discoveryProperties.setSupportedSubjectTypes(ImmutableList.of("public","pairwise"));
  discoveryProperties.setSupportedClaimTypes(ImmutableList.of("normal"));
  discoveryProperties.setSupportedGrantTypes(ImmutableList.of("authorization_code","password","implicit"));
  return discoveryProperties;
}
