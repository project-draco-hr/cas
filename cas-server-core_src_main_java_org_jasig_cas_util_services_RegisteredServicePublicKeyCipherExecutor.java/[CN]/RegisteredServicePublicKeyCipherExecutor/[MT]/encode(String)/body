{
  try {
    final PublicKey publicKey=createRegisteredServicePublicKey();
    final byte[] result=encodeInternal(data,publicKey);
    if (result != null) {
      return CompressionUtils.encodeBase64(result);
    }
  }
 catch (  final Exception e) {
    throw new RuntimeException(e);
  }
  return null;
}
