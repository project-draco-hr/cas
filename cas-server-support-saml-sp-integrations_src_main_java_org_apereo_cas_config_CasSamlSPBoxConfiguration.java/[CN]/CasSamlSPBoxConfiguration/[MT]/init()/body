{
  final SamlRegisteredService service=SamlSPUtils.newSamlServiceProviderService(casProperties.getSamlSP().getBox().getName(),casProperties.getSamlSP().getBox().getDescription(),casProperties.getSamlSP().getBox().getMetadata(),casProperties.getSamlSP().getBox().getAttributes());
  if (service != null) {
    service.setSignResponses(true);
    servicesManager.save(service);
    servicesManager.load();
  }
}
