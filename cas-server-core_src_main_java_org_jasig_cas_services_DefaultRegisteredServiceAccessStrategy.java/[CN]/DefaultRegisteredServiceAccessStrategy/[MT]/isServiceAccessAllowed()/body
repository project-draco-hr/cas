{
  if (!this.enabled) {
    logger.trace("Service is not enabled in service registry.");
  }
  final DateTime now=DateTime.now();
  if (this.startingDateTime != null) {
    final DateTime st=DateTime.parse(this.startingDateTime);
    if (now.isBefore(st)) {
      logger.warn("Service access not allowed because it starts at {}. Now is {}",this.startingDateTime,now);
      return false;
    }
  }
  if (this.endingDateTime != null) {
    final DateTime et=DateTime.parse(this.endingDateTime);
    if (now.isAfter(et)) {
      logger.warn("Service access not allowed because it ended at {}. Now is {}",this.endingDateTime,now);
      return false;
    }
  }
  return this.enabled;
}
