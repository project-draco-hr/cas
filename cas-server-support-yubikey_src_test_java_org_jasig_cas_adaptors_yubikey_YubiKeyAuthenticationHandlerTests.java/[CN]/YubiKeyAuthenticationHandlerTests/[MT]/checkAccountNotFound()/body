{
  final YubiKeyAuthenticationHandler handler=new YubiKeyAuthenticationHandler(CLIENT_ID,SECRET_KEY);
  handler.setRegistry(new YubiKeyAccountRegistry(){
    @Override public boolean isYubiKeyRegisteredFor(    final String uid,    final String yubikeyPublicId){
      return false;
    }
  }
);
  handler.authenticate(new UsernamePasswordCredential("casuser",OTP));
}
