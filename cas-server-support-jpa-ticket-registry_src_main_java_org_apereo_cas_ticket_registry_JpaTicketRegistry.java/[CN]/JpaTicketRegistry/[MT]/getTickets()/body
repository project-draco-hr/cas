{
  final List<TicketGrantingTicketImpl> tgts=this.entityManager.createQuery("select t from TicketGrantingTicketImpl t",TicketGrantingTicketImpl.class).getResultList();
  final List<ServiceTicketImpl> sts=this.entityManager.createQuery("select s from ServiceTicketImpl s",ServiceTicketImpl.class).getResultList();
  final List<Ticket> tickets=new ArrayList<>(tgts);
  tickets.addAll(sts);
  return tickets;
}
