{
  if (StringUtils.isBlank(serviceId) || StringUtils.isBlank(name) || StringUtils.isBlank(description)) {
    throw new IllegalArgumentException("Service name/description/id is missing");
  }
  final RegexRegisteredService service=new RegexRegisteredService();
  service.setServiceId(serviceId);
  service.setDescription(description);
  service.setName(name);
  service.setEvaluationOrder(evaluationOrder);
  service.setAccessStrategy(new DefaultRegisteredServiceAccessStrategy(enabled,ssoEnabled));
  return service;
}
