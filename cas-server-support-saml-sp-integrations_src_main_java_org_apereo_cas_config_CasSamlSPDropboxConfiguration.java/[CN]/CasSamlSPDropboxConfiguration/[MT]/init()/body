{
  final SamlRegisteredService service=SamlSPUtils.newSamlServiceProviderService(casProperties.getSamlSP().getDropbox().getName(),casProperties.getSamlSP().getDropbox().getDescription(),casProperties.getSamlSP().getDropbox().getMetadata(),casProperties.getSamlSP().getDropbox().getNameIdAttribute());
  if (service != null) {
    service.setSignResponses(true);
    servicesManager.save(service);
    servicesManager.load();
  }
}
