{
  if (registeredService == null) {
    final String msg=String.format("Unauthorized Service Access. Service [%s] is not found in service registry.",service.getId());
    LOGGER.warn(msg);
    throw new UnauthorizedServiceException(UnauthorizedServiceException.CODE_UNAUTHZ_SERVICE,msg);
  }
  if (!registeredService.getAccessStrategy().isServiceAccessAllowed()) {
    final String msg=String.format("Unauthorized Service Access. Service [%s] is not enabled in service registry.",service.getId());
    LOGGER.warn(msg);
    throw new UnauthorizedServiceException(UnauthorizedServiceException.CODE_UNAUTHZ_SERVICE,msg);
  }
}
