{
  try {
    final URL configUrl=this.configLocation.getURL();
    final Config config=new XmlConfigBuilder(this.configLocation.getInputStream()).build();
    config.setConfigurationUrl(configUrl);
    config.setInstanceName(this.getClass().getSimpleName()).setProperty("hazelcast.logging.type","slf4j").setProperty("hazelcast.max.no.heartbeat.seconds","5");
    return Hazelcast.newHazelcastInstance(config);
  }
 catch (  final Exception e) {
    throw new RuntimeException(e);
  }
}
