{
  logger.info("Setting up Hazelcast Ticket Registry...");
  logger.debug("Hazelcast instance: {} with name {}",this.hz,this.registry.getName());
  logger.debug("TGT timeout: [{}s]",this.ticketGrantingTicketTimoutInSeconds);
