{
  final int totalItems=100;
  for (int i=0; i < totalItems; i++) {
    this.decorator.put("key" + i,"value" + i);
  }
  assertEquals(this.decorator.size(),totalItems);
  for (int i=0; i < totalItems; i++) {
    assertNull(this.map.get("key" + i));
    assertEquals("value" + i,this.decorator.get("key" + i));
  }
}
