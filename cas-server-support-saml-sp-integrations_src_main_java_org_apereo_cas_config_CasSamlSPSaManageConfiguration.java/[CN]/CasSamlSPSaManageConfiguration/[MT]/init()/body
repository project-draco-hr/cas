{
  final SamlRegisteredService service=SamlSPUtils.newSamlServiceProviderService(casProperties.getSamlSP().getSaManage().getName(),casProperties.getSamlSP().getSaManage().getDescription(),casProperties.getSamlSP().getSaManage().getMetadata(),casProperties.getSamlSP().getSaManage().getNameIdAttribute());
  if (service != null) {
    service.setSignResponses(true);
    servicesManager.save(service);
    servicesManager.load();
  }
}
