{
  final Matcher matcher=TICKET_ID_PATTERN.matcher(msg);
  if (matcher.find()) {
    final String match=matcher.group();
    final String newId=matcher.group(1) + "-" + StringUtils.repeat("*",match.length() - VISIBLE_ID_TAIL_LENGTH)+ StringUtils.right(match,VISIBLE_ID_TAIL_LENGTH);
    return msg.replaceAll(match,newId);
  }
  return msg;
}
