{
  final TicketGrantingTicket tId=getCentralAuthenticationService().createTicketGrantingTicket(TestUtils.getCredentialsWithSameUsernameAndPassword());
  final ServiceTicket sId=getCentralAuthenticationService().grantServiceTicket(tId.getId(),TestUtils.getService());
  getCentralAuthenticationService().destroyTicketGrantingTicket(tId.getId());
  final MockHttpServletRequest request=new MockHttpServletRequest();
  request.addParameter("service",TestUtils.getService().getId());
  request.addParameter("ticket",sId.getId());
  assertEquals(ServiceValidateController.DEFAULT_SERVICE_FAILURE_VIEW_NAME,this.serviceValidateController.handleRequestInternal(request,new MockHttpServletResponse()).getViewName());
}
