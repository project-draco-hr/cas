{
  for (  Resource r : this.resources) {
    logger.debug("Fetching CRL data from {}",r);
    try {
      this.crlRevocationChecker.addCrl(CertUtils.fetchCRL(r));
    }
 catch (    final Exception e) {
      if (throwOnError) {
        throw new RuntimeException("Error fetching CRL from " + r,e);
      }
    }
  }
}
