{
  final AccountState state=response.getAccountState();
  if (state != null) {
    if (state.getError() != null) {
      AccountPasswordMustChangeException.checkAndThrowException(state.getError().getCode(),state.getError().getMessage());
      InvalidLoginLocationException.checkAndThrowException(state.getError().getCode(),state.getError().getMessage());
      InvalidLoginTimeException.checkAndThrowException(state.getError().getCode(),state.getError().getMessage());
    }
  }
  super.examineAccountState(response);
}
