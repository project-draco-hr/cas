{
  final CredentialMetaData meta=new BasicCredentialMetaData(new UsernamePasswordCredential());
  final AuthenticationHandler handler=new SimpleTestUsernamePasswordAuthenticationHandler();
  final DefaultAuthenticationBuilder builder=new DefaultAuthenticationBuilder(TestUtils.getPrincipal()).addCredential(meta).addSuccess("test",new DefaultHandlerResult(handler,meta));
  if (this.p.supports(credential)) {
    this.p.populateAttributes(builder,credential);
  }
  return builder;
}
