{
  final TicketGrantingTicket ticketGrantingTicketId=this.remoteCentralAuthenticationService.createTicketGrantingTicket(TestUtils.getCredentialsWithSameUsernameAndPassword());
  this.remoteCentralAuthenticationService.grantServiceTicket(ticketGrantingTicketId.getId(),TestUtils.getService(),TestUtils.getCredentialsWithSameUsernameAndPassword());
}
