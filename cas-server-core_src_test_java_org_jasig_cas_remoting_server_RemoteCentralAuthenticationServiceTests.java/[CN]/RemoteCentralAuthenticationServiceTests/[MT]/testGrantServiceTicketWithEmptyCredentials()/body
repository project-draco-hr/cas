{
  final String ticketGrantingTicketId=this.remoteCentralAuthenticationService.createTicketGrantingTicket(TestUtils.getCredentialsWithSameUsernameAndPassword());
  try {
    this.remoteCentralAuthenticationService.grantServiceTicket(ticketGrantingTicketId,TestUtils.getService(),TestUtils.getCredentialsWithDifferentUsernameAndPassword("",""));
    fail("IllegalArgumentException expected.");
  }
 catch (  final IllegalArgumentException e) {
    return;
  }
}
