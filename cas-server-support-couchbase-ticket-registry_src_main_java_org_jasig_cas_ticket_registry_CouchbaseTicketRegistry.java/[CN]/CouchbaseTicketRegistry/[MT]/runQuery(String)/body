{
  final ViewResult allKeys=couchbase.bucket().query(ViewQuery.from(UTIL_DOCUMENT,"all_tickets").startKey(prefix).endKey(prefix + END_TOKEN).reduce());
  final Iterator<ViewRow> iterator=allKeys.iterator();
  if (iterator.hasNext()) {
    final ViewRow res=iterator.next();
    return (Integer)res.value();
  }
 else {
    return 0;
  }
}
