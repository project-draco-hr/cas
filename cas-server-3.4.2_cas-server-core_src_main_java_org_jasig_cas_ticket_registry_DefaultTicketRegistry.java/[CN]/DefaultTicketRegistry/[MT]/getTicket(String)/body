{
  if (ticketId == null) {
    return null;
  }
  if (log.isDebugEnabled()) {
    log.debug("Attempting to retrieve ticket [" + ticketId + "]");
  }
  final Ticket ticket=this.cache.get(ticketId);
  if (ticket != null) {
    log.debug("Ticket [" + ticketId + "] found in registry.");
  }
  return ticket;
}
