{
  final PrincipalAttributesRepository repository=new CachingPrincipalAttributesRepository(this.dao,Collections.<String,Object>singletonMap("mail","final@example.com"),TimeUnit.SECONDS,5);
  assertEquals(repository.getAttributes("uid").size(),1);
  assertTrue(repository.getAttributes("uid").containsKey("mail"));
  attributes.clear();
  assertTrue(repository.getAttributes("uid").containsKey("mail"));
  ((Closeable)repository).close();
}
