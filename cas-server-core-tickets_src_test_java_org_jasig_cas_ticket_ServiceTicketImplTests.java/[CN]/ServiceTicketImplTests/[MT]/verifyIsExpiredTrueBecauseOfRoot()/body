{
  final TicketGrantingTicket t=new TicketGrantingTicketImpl("test",org.jasig.cas.authentication.TestUtils.getAuthentication(),new NeverExpiresExpirationPolicy());
  final ServiceTicket s=t.grantServiceTicket(this.uniqueTicketIdGenerator.getNewTicketId(ServiceTicket.PREFIX),org.jasig.cas.services.TestUtils.getService(),new NeverExpiresExpirationPolicy(),false,true);
  t.markTicketExpired();
  assertTrue(s.isExpired());
}
