{
  StringWriter sw=new StringWriter();
  String xml="";
  try {
    JAXBContext context=JAXBContext.newInstance(this.getClass());
    Marshaller marshaller=context.createMarshaller();
    marshaller.marshal(this,sw);
    xml=sw.toString();
  }
 catch (  Exception e) {
    logger.error(e.getMessage(),e);
  }
  return xml;
}
