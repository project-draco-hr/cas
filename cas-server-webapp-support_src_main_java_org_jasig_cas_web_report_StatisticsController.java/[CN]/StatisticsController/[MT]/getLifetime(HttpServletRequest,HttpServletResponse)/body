{
  final Map<String,Object> responseMap=new HashMap<>();
  responseMap.put("startTime",this.upTimeStartDate);
  final double difference=System.currentTimeMillis() - this.upTimeStartDate.getTime();
  responseMap.put("upTime",calculateUptime(difference,new LinkedList<Integer>(Arrays.asList(NUMBER_OF_MILLISECONDS_IN_A_DAY,NUMBER_OF_MILLISECONDS_IN_AN_HOUR,NUMBER_OF_MILLISECONDS_IN_A_MINUTE,NUMBER_OF_MILLISECONDS_IN_A_SECOND,1)),new LinkedList<String>(Arrays.asList("day","hour","minute","second","millisecond"))));
  return responseMap;
}
