{
  String principalId=principal.getId();
  final Map<String,Object> attributes=getPrincipalAttributes(principal);
  if (attributes.containsKey(this.usernameAttribute)) {
    principalId=attributes.get(this.usernameAttribute).toString();
  }
 else {
    logger.warn("Principal [{}] does not have an attribute [{}] among attributes [{}] so CAS cannot " + "provide the user attribute the service expects. " + "CAS will instead return the default principal id [{}]",principalId,this.usernameAttribute,attributes,principalId);
  }
  logger.debug("Principal id to return for [{}] is [{}]. The default principal id is [{}].",service.getId(),principalId,principal.getId());
  return principalId;
}
