{
  final String[] aliases=this.applicationContext.getAutowireCapableBeanFactory().getAliases("inMemoryServiceRegistryDao");
  if (aliases.length > 0) {
    LOGGER.debug("{} is used as the active service registry dao",this.getClass().getSimpleName());
    try {
      final List<RegisteredService> list=(List<RegisteredService>)this.applicationContext.getBean("inMemoryRegisteredServices",List.class);
      if (list != null) {
        LOGGER.debug("Loaded {} services from the application context for {}",list.size(),this.getClass().getSimpleName());
        this.registeredServices=list;
      }
    }
 catch (    final Exception e) {
      LOGGER.debug("No registered services are defined for {}",this.getClass().getSimpleName());
    }
  }
}
