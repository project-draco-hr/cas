{
  final String logoutRequest=this.logoutMessageBuilder.create(request);
  request.getService().setLoggedOutAlready(true);
  LOGGER.debug("Sending logout request for: [{}]",request.getService().getId());
  return this.httpClient.sendMessageToEndPoint(request.getService().getOriginalUrl(),logoutRequest,this.issueAsynchronousCallbacks);
}
