{
  final boolean checkParameterExist=checkParameterExist(request,OAuthConstants.CLIENT_ID) && checkParameterExist(request,OAuthConstants.REDIRECT_URI) && checkParameterExist(request,OAuthConstants.CLIENT_SECRET)&& checkParameterExist(request,OAuthConstants.CODE);
  return checkParameterExist && checkServiceValid(request) && checkCallbackValid(request)&& checkClientSecret(request);
}
