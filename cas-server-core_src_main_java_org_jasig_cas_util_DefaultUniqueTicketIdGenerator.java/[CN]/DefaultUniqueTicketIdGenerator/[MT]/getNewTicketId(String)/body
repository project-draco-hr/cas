{
  final String number=this.numericGenerator.getNextNumberAsString();
  final StringBuilder buffer=new StringBuilder(prefix.length() + 2 + (this.suffix != null ? this.suffix.length() : 0)+ this.randomStringGenerator.getMaxLength()+ number.length());
  buffer.append(prefix);
  buffer.append("-");
  buffer.append(number);
  buffer.append("-");
  buffer.append(this.randomStringGenerator.getNewString());
  if (this.suffix != null) {
    buffer.append(this.suffix);
  }
  return buffer.toString();
}
