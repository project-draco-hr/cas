{
  assertEquals(a.size(),b.size());
  Object valueA;
  List<Object> valueB;
  for (  String key : a.keySet()) {
    valueA=a.get(key);
    valueB=b.get(key);
    if (valueA instanceof List) {
      for (      Object item : ((List)valueA)) {
        assertTrue(valueB.contains(item));
      }
    }
 else {
      assertEquals(1,valueB.size());
      assertEquals(valueA,valueB.get(0));
    }
  }
}
