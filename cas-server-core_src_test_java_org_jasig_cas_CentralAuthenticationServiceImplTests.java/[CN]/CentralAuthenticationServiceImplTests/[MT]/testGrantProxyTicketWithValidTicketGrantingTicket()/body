{
  final String ticketId=getCentralAuthenticationService().createTicketGrantingTicket(TestUtils.getCredentialsWithSameUsernameAndPassword());
  final String serviceTicketId=getCentralAuthenticationService().grantServiceTicket(ticketId,TestUtils.getService());
  final String pgt=getCentralAuthenticationService().delegateTicketGrantingTicket(serviceTicketId,TestUtils.getHttpBasedServiceCredentials());
  final String pt=getCentralAuthenticationService().grantServiceTicket(pgt,TestUtils.getService(),null);
  assertTrue(pt.startsWith(ServiceTicket.PROXY_TICKET_PREFIX));
}
