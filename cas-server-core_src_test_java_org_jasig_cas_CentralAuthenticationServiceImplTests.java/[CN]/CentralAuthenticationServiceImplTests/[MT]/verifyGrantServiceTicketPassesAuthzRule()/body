{
  final AuthenticationContext ctx=TestUtils.getAuthenticationContext(getAuthenticationSystemSupport(),getService("TestServiceAttributeForAuthzPasses"));
  final TicketGrantingTicket ticketId=getCentralAuthenticationService().createTicketGrantingTicket(ctx);
  getCentralAuthenticationService().grantServiceTicket(ticketId.getId(),getService("TestServiceAttributeForAuthzPasses"),ctx);
}
