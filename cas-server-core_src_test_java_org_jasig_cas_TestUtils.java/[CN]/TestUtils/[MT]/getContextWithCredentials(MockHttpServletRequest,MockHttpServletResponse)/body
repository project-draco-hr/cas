{
  final MockRequestContext context=getContext(request,response);
  context.getRequestScope().put(CONST_CREDENTIALS,TestUtils.getCredentialsWithSameUsernameAndPassword());
  context.getRequestScope().put(CONST_WEBFLOW_BIND_EXCEPTION,new BindException(TestUtils.getCredentialsWithSameUsernameAndPassword(),CONST_CREDENTIALS));
  return context;
}
