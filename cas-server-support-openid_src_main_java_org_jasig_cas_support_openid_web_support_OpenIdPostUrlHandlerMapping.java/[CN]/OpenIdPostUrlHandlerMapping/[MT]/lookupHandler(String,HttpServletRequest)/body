{
  if (HttpMethod.POST.name().equals(request.getMethod()) && (OpenIdConstants.CHECK_AUTHENTICATION.equals(request.getParameter(OpenIdConstants.OPENID_MODE)) || OpenIdConstants.ASSOCIATE.equals(request.getParameter(OpenIdConstants.OPENID_MODE)))) {
    return super.lookupHandler(urlPath,request);
  }
  return null;
}
