{
  final HttpSession session=request.getSession();
  if (authnRequest != null) {
    logger.debug("Storing authentication request issued by {} into session as {}",authnRequest.getIssuer().getValue(),AuthnRequest.class.getName());
  }
 else {
    logger.debug("Removing authentication request from session");
  }
  session.setAttribute(AuthnRequest.class.getName(),authnRequest);
}
